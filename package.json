{
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@vitest/coverage-v8": "^2.0.5",
    "babel-loader": "^9.1.3",
    "happy-dom": "^14.12.3",
    "php-cgi-wasm": "0.0.9-alpha-2",
    "php-wasm": "0.0.9-alpha-13",
    "php-wasm-dom": "0.0.0-h",
    "php-wasm-gd": "^0.0.9-j",
